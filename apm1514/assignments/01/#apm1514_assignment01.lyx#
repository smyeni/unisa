\begin_layout Standard
\begin_inset Formula $\frac{dy}{dt}$
\end_inset

 = 2(4 - y)
\end_layout

\begin_layout Standard
\begin_inset Formula $\Rightarrow$
\end_inset


\begin_inset Formula $\frac{dy}{4-y}$
\end_inset

 = 2dt
\end_layout

\begin_layout Standard
\begin_inset Formula $\Rightarrow\int\frac{dy}{4-y}$
\end_inset

 = 
\begin_inset Formula $\int2dt$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\Rightarrow$
\end_inset

-ln|4-y| = 2t + C
\end_layout

\begin_layout Standard
\begin_inset Formula $\Rightarrow$
\end_inset

ln 
\begin_inset Formula $\left|4-y\right|$
\end_inset

 = -2t + C
\end_layout

\begin_layout Standard
\begin_inset Formula $\Rightarrow$
\end_inset

4-y = e
\begin_inset Formula $^{-2t+C}$
\end_inset

 = Ae
\begin_inset Formula $^{-2t}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\Rightarrow$
\end_inset

-y = -4 + Ae
\begin_inset Formula $^{-2t}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\therefore$
\end_inset

y = 4 + Ae
\begin_inset Formula $^{-2t}$
\end_inset


\end_layout

